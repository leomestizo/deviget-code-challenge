{
  "extends": [
    "airbnb-base",
    "airbnb/rules/react"
  ],
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./config/webpack/base.config.js"
      }
    }
  },
  "rules": {
    "quotes": ["error", "double"],
    "react/button-has-type": "off",
    "react/jsx-fragments": "off"
  }
}
